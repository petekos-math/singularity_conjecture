@Article{axioms6020007,
	AUTHOR = {Herr, John E. and Weber, Eric S.},
	TITLE = {Fourier Series for Singular Measures},
	JOURNAL = {Axioms},
	VOLUME = {6},
	YEAR = {2017},
	NUMBER = {2},
	ARTICLE-NUMBER = {7},
	URL = {https://www.mdpi.com/2075-1680/6/2/7},
	ISSN = {2075-1680},
	ABSTRACT = {Using the Kaczmarz algorithm, we prove that for any singular Borel probability measure μ on [ 0 , 1 ) , every f ∈ L 2 ( μ ) possesses a Fourier series of the form f ( x ) = ∑ n = 0 ∞ c n e 2 π i n x . We show that the coefficients c n can be computed in terms of the quantities f ^ ( n ) = ∫ 0 1 f ( x ) e − 2 π i n x d μ ( x ) . We also demonstrate a Shannon-type sampling theorem for functions that are in a sense μ -bandlimited.},
	DOI = {10.3390/axioms6020007}
}





@article{strichartzI,
	ISSN = {00222518, 19435258},
	URL = {http://www.jstor.org/stable/24895596},
	abstract = {A self-similar measure on Rn was defined by Hutchinson to be a probability measure satisfying $\mu =\sum_{j=1}^{m}a_j\mu \circ S^{-1}_j$ where Sjx = ρjRjx + bj is a contractive similarity (0 < ρj < 1, Rj orthogonal) and 0 < aj < 1, $\Sigma ^m_{j=1}a_j=1$. Examples include the usual Cantor measure. Associated to μ is a self-similarity dimension $\alpha =\sum_{j=1}^{m}a_jloga_j/\sum_{j=1}^{m}a_jlog\rho_j$. We prove that μ is supported on a set of dimension α under certain additional hypotheses, and that no set of smaller dimension supports μ. We establish the asymptotic behavior of averages of the Fourier transform of μ such as $\int _{|x|\leq R}|\hat{\mu}(x)|^2dx=O(R^{n-\beta})$ as R → ∞ or, more generally, $\int _{|x|\leq R}|\hat{\mu}(x)|^{2k}dx=O(R^{n-\beta_{k}})$ as R → ∞, again under various addition hypotheses.},
	author = {Robert S. Strichartz},
	journal = {Indiana University Mathematics Journal},
	number = {3},
	pages = {797--817},
	publisher = {Indiana University Mathematics Department},
	title = {Self-Similar Measures and Their Fourier Transforms I},
	urldate = {2024-03-12},
	volume = {39},
	year = {1990}
}



